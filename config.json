{
  "Listen": ":8080",
  "Handlers": [
    {
      "HandlerType": "static",
      "HandlerRoute": "/",
      "resFile": "~/simple_form.html"
    },
    {
      "HandlerType": "form",
      "HandlerRoute": "/submit",
      "baseURL": "parentdomain.com",
      "confPath": "~/vhosts.d",
      "confExt": ".conf",
      "excludes": [
        "moxxi",
        "backend",
        "secure"
      ],
      "confFile": "~/config.template",
      "resFile": "~/response.template",
      "subdomainLen": 8
    },
    {
      "HandlerType": "json",
      "HandlerRoute": "/json",
      "baseURL": "parentdomain.com",
      "confPath": "~/vhosts.d",
      "confExt": ".conf",
      "excludes": [
        "moxxi",
        "backend",
        "secure"
      ],
      "confFile": "~/config.template",
      "resFile": "~/response.template",
      "subdomainLen": 8
    }
  ]
}